#include <unisdt.h>

int give_values(*tab)
{
	int	i;

	i = 0;
	while(tab[i] != '\0')
		{
			i++;
		}
		return (i);
		return (*tab);
}




void ft_rev_int_tab(int *tab, int size)
{
	while( size-- > 0)
	{
	write(1, tab[size--],1) 
	}
}

int main	(void)
{
	int	tab;

	tab[] = "Rod & Lua";
	give_values(&tab);
}